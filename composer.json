{
    "name": "cineca/translationbundle",
    "description": "translation bundle from Database",
    "type": "symfony-bundle",
    "authors": [{
        "name": "nephthali djabon tchounda",
        "email": "neph@cineca.it"
    }],
    "keywords": [
        "translation bundle"
    ],
    "license": [
        "MIT"
    ],
    "repositories": [{
        "type": "composer",
        "url": "http://miur-home.dev.cineca.it/satis/html"
    }],
    "require": {
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "~2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "knplabs/knp-paginator-bundle": "~2.5"
    },
    "autoload": {
        "psr-4": {
            "Cineca\\TranslationBundle\\": "Cineca/TranslationBundle/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1.0-dev",
            "dev-devel": "v1.0-dev-devel"
        }
    }
}