{
    "name": "cineca/translationbundle",
    "description": "translation bundle from Database",
    "type": "symfony-bundle",
    "authors": [{
        "name": "nephthali djabon tchounda",
        "email": "neph@cineca.it"
    }],
    "keywords": [
        "translation bundle"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4"
    },
    "autoload": {
        "psr-0": {
            "Cineca\\TranslationBundle": ""
        }
    },
    "target-dir": "Cineca/TranslationBundle",
    "repositories": [{
        "type": "git",
        "url": "https://git-miur.cineca.it/ndjabont/TranslationBundle.git"
    }],
    "extra": {
        "branch-alias": {
            "dev-master": "v1.0-dev"
        }
    }
}